<script>

  $(".mobile-btn").click(function(){

    if($(this).hasClass("close")){
      $(this).removeClass("close")
      $(".m-nav").fadeOut();
    }else{
      $(this).addClass("close")
      $(".m-nav").fadeIn();
    }


  });

</script>
