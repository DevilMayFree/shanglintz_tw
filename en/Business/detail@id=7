            <script type="text/javascript">
                    $('#title').click(function(){
                           $(".search").css("border","1px solid #ddd");
                    });

                $('#title').focus(function(){
                    if($.trim($('#title').val()) == 'Please enter a keyword'){
                        $('#title').val('');
                        $('#title').css("color","#999");
                    }
                });

                $('#searchBtn').click(function(){
                    if(!$.trim($('#title').val()) || $.trim($('#title').val()) == 'Please enter a keyword'){
                        $('#title').val('Please enter a keyword');
                        $('#title').css("color","#B48F32");
                    }else{
                        $('#searchForm').submit();
                    }
                });
                    var navVr = document.querySelectorAll('.nav_bar');
    window.onscroll = function(){
        var t = document.documentElement.scrollTop || document.body.scrollTop;
        for (var i = 0; i < navVr.length; i++) {
            navVr[i].style.top='0px';
        };
    }
                    function changeNavHeight(){
                    	var h = $(window).height();
                        $('.contains').css('height',h);
                    }
                    $(function(){
                        $(window).on('resize',function(){changeNavHeight()});
                        changeNavHeight();
                    })
            </script>
